<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Deno App</title>
</head>

<body>
  <table>
    <tr>
      <th>id</th>
      <th>message</th>
    </tr>
    <% it.fortunes.forEach(function(fortune) { %>
      <tr>
        <td>
          <%_=fortune.id _%>
        </td>
        <td>
          <%_=fortune.message _%>
        </td>
      </tr>
      <% }) %>
  </table>
</body>

</html>